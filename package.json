{
  "name": "my-vue-app",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "concurrently \"vite\" \"yarn server-dev\"",
    "server-dev": "concurrently \"rollup -c rollup.config.js -w\" \"nodemon dist/server/server.js\"",
    "build": "tsc && vite build && rollup -c rollup.config.js",
    "preview": "concurrently \"vite preview\" \"nodemon dist/server/server.js\""
  },
  "dependencies": {
    "@prisma/client": "^4.0.0",
    "@tanstack/react-query": "^4.0.10",
    "axios": "^0.27.2",
    "case": "^1.6.3",
    "express": "^4.18.1",
    "prisma": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-run": "^2.1.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@types/express": "^4.17.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^1.3.0",
    "concurrently": "^7.3.0",
    "esbuild": "^0.14.49",
    "nodemon": "^2.0.19",
    "typescript": "^4.6.3",
    "vite": "^2.9.9"
  }
}
